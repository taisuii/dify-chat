      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dist/index.js": "./dist/index.js",
    "./theme-default.css": "./dist/assets/theme-default.css",
    "./markdown-styles.css": "./dist/assets/markdown-renderer.css"
  },
