    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "rslib build && node scripts/inject-react-import.mjs",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "lint": "eslint ."
  },
