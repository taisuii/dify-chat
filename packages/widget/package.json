        "directory": "packages/widget"
    },
    "source": "./src/index.ts",
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "source": "./src/index.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
